{
  "name": "badminton-tournament",
  "productName": "badminton-tournament",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "Kaydesu",
    "email": "57744117+Kaydesu@users.noreply.github.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "badminton_tournament"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderers/index.html",
                  "js": "./src/renderers/main/index.tsx",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/electron/bridge.ts"
                  }
                },
                {
                  "html": "./src/renderers/index.html",
                  "js": "./src/renderers/tournamentCreation/index.tsx",
                  "name": "tournament_creation",
                  "preload": {
                    "js": "./src/electron/bridge.ts"
                  }
                },
                {
                  "html": "./src/renderers/index.html",
                  "js": "./src/renderers/teamManagement/index.tsx",
                  "name": "team_management",
                  "preload": {
                    "js": "./src/electron/bridge.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "6.0.0-beta.63",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.23",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "css-loader": "^6.0.0",
    "electron": "17.0.1",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "konva": "^8.3.4",
    "node-loader": "^2.0.0",
    "nodemon": "^2.0.15",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.2",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "antd": "^4.19.0",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "styled-components": "^5.3.3",
    "uuid": "^8.3.2",
    "webpack": "^5.70.0"
  }
}
